{
  "fmt": {
    "options": {
      "indentWidth": 2,
      "lineWidth": 80,
      "semiColons": false
    }
  },
  "importMap": "./import_map.json",
  "lock": false,
  "compilerOptions": {
    "lib": ["DOM", "ESNext", "deno.ns"],
    "allowUnreachableCode": true,
    "allowJs": true,
    "noImplicitAny": false,
    "checkJs": false
  },
  "tasks": {
    "dev": "deno run -A deploy.ts --src=./examples/src/app.ts --appRoute=/examples/app.js --dev=true"
  }
}
